<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hoanglm.rxandroidjmdns.RxMergeActivity">

    <Button
        android:id="@+id/merge_with_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Merge With"/>

    <Button
        android:id="@+id/merge_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Merge"
        app:layout_constraintTop_toBottomOf="@+id/merge_with_button"/>

    <Button
        android:id="@+id/switch_merge_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Switch Merge"
        app:layout_constraintTop_toBottomOf="@+id/merge_button"/>
</android.support.constraint.ConstraintLayout>
